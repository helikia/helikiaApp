{
  "name": "helikia-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.16.3"
  },
  "scripts": {
    "start": "APP_ENV=https://helikia-staging.herokuapp.com PORT=5000 node --experimental-modules ./packages/api",
    "serve": "APP_ENV=http://localhost vue-cli-service serve ./packages/app/src/main.js",
    "dev:back": "APP_ENV=http://localhost nodemon --inspect --experimental-modules ./packages/api --watch ./packages/api --ext mjs,js,jsx,json",
    "loadFixtures": "nodemon --experimental-modules ./packages/api/src/modules/mongodb/fixtures/fixtures.mjs",
    "build": "NODE_ENV=production vue-cli-service build ./packages/app/src/main.js --modern",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e ./packages/app/src/main.js",
    "test:e2e:prod": "vue-cli-service test:e2e ./packages/app/src/main.js --headless",
    "test:unit": "vue-cli-service test:unit",
    "storybook:dev": "cd ./packages/storybook && vue-cli-service storybook:serve -p 6006 -c config/storybook"
  },
  "dependencies": {
    "@hapi/boom": "^7.4.3",
    "@hapi/joi": "^15.1.1",
    "apollo-server": "^2.9.0",
    "bcrypt": "^3.0.6",
    "deepmerge": "^4.0.0",
    "good": "^8.1.2",
    "good-console": "^8.0.0",
    "good-squeeze": "^5.1.0",
    "hapi": "^18.1.0",
    "jsonwebtoken": "^8.5.1",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.15",
    "apollo-server-hapi": "^2.9.3",
    "core-js": "^2.6.5",
    "glue": "^6.0.0",
    "graphql": "^14.5.3",
    "moment": "^2.24.0",
    "mongodb": "^3.3.2",
    "sass": "^1.22.10",
    "vee-validate": "^3.0.3",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-rc.2",
    "vue-i18n": "^8.14.0",
    "vue-router": "^3.1.2",
    "vuetify": "^2.0.18",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^4.2.95",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "graphql-tag": "^2.10.1",
    "lint-staged": "^8.1.5",
    "material-design-icons-iconfont": "^5.0.1",
    "sass-loader": "^7.0.0",
    "vue-cli-plugin-apollo": "^0.21.0",
    "vue-template-compiler": "^2.6.10"
  }
}